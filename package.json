{
	"name": "spade-language-support",
	"displayName": "Spade Language Support",
	"description": "Syntax highlighting for Spade",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.40.0"
	},
	"publisher": "kcza",
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "spade",
				"aliases": [
					"Spade",
					"spade"
				],
				"extensions": [
					".sp"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "spade",
				"scopeName": "source.spade",
				"path": "./syntaxes/spade.tmLanguage.json"
			}
		]
	},
	"icon": "img/Spade128.png"
}
