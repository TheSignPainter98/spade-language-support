{
    "name": "spade-language-support",
    "displayName": "Spade Language Support",
    "description": "Syntax highlighting for Spade",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "spade",
                "aliases": [
                    "Spade",
                    "spade"
                ],
                "extensions": [
                    ".sp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "spade",
                "scopeName": "source.spade",
                "path": "./syntaxes/spade.tmLanguage.json"
            }
        ]
    }
}
